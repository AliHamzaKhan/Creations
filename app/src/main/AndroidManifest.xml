<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aaa.campussystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogInActivtty" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".Activities.AdminActivity" />
        <activity android:name=".Activities.StudentActivity" />
        <activity android:name=".Activities.CompanyActivity" />
        <activity android:name=".Activities.StudentProfileActivity" />
        <activity android:name=".Activities.CompanyProfileActivity" />
        <activity android:name=".Activities.CompanyPostAJobActivity" />
        <activity android:name=".Activities.CompanyMoveToActivity"></activity>
    </application>

</manifest>